                android:numeric="integer"
                android:padding="1dp" />
            <EditText
                android:text="24"
                android:id="@+id/s_shape_text24"
                android:layout_column="8"
                android:layout_gravity="right"
                android:inputType="number|numberDecimal"
                android:textSize="10dp"
                android:numeric="integer"
                android:padding="1dp" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:padding="1dp">
            <EditText
                android:text="25"
                android:id="@+id/s_shape_text25"
                android:layout_column="1"
                android:autoText="false"
                android:inputType="number|numberDecimal"
                android:textSize="10dp"
                android:numeric="integer"
                android:padding="1dp" />

            <EditText
                android:text="26"
                android:id="@+id/s_shape_text26"
                android:layout_column="2"
                android:layout_gravity="right"
                android:autoText="false"
                android:inputType="number|numberDecimal"
                android:textSize="10dp"
                android:numeric="integer"
                android:padding="1dp" />

            <EditText
                android:text="27"
                android:id="@+id/s_shape_text27"
                android:layout_column="3"
                android:layout_gravity="right"
