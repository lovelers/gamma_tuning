<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="media.willis.com.gamma_tuning.gamma_s_shape">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/tableLayout">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:padding="1dp">
            <EditText
                android:text="1"
                android:id="@+id/s_shape_text1"
                android:layout_column="1"
                android:editable="true"
                android:gravity="fill_horizontal"
                android:textIsSelectable="true"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />

            <EditText
                android:text="2"
                android:id="@+id/s_shape_text2"
                android:layout_column="2"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="3"
                android:id="@+id/s_shape_text3"
                android:layout_column="3"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="4"
                android:id="@+id/s_shape_text4"
                android:layout_column="4"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="5"
                android:id="@+id/s_shape_text5"
                android:layout_column="5"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="6"
                android:id="@+id/s_shape_text6"
                android:layout_column="6"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="7"
                android:id="@+id/s_shape_text7"
                android:layout_column="7"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
            <EditText
                android:text="8"
                android:id="@+id/s_shape_text8"
                android:layout_column="8"
                android:layout_gravity="right"
                android:gravity="fill_horizontal"
                android:textSize="10dp"
                android:padding="1dp"
                android:inputType="number|numberDecimal" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="9"
                android:id="@+id/s_shape_text9"
                android:layout_column="1"
                android:inputType="number|numberDecimal"
                android:numeric="integer"
                android:textSize="10dp"
                android:padding="1dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="10"
                android:id="@+id/s_shape_text10"
                android:layout_column="2"
                android:layout_gravity="right"
                android:inputType="number|numberDecimal"
                android:numeric="integer"
                android:textSize="10dp"
                android:padding="1dp" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="11"
                android:id="@+id/s_shape_text11"
